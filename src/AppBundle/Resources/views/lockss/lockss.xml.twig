<lockss-config>
    {% for key, value in pln.properties -%}
    {% if value is iterable -%}
    <property name="{{ key }}">
        <list>
        {% for v in value -%}
            <value>{{ v }}</value>
        {% endfor -%}
        </list>
    </property>
    {% else -%}
    <property name="{{ key }}" value="{{ value }}"/>
    {% endif -%}
    {% endfor -%}
</lockss-config>
